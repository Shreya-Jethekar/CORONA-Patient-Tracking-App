<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.example.p3">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:networkSecurityConfig="@xml/network_security_config"
        android:roundIcon="@mipmap/ic_launcher_round"
        android:supportsRtl="true"
        android:theme="@style/Theme.P3">
        <activity
            android:name=".AffectedCountries"
            android:exported="true" />
        <activity
            android:name=".Search_BY"
            android:exported="true" />
        <activity
            android:name=".Home_Of_Login"
            android:exported="true"
            android:label="@string/title_activity_home_of_login"
            android:theme="@style/Theme.P3.NoActionBar" />
        <activity android:name=".Register_screen" />
        <activity android:name=".Login_screen" />
        <activity
            android:name=".Splash_screen"
            android:exported="true">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity android:name=".MainActivity" />
    </application>

</manifest>